{% extends "Templates/default.twig" %}

{% block body %}

	<a href="{{ url("posts.create") }}">
		C
	</a>

	<table>
		<tbody>
			{% for post in posts %}
				<tr>
					<td>
						{{ post.getId }}
					</td>
					<td>
						{{ dump(post.getPostBlockFiles) }}
					</td>
					<td>
						<a href="{{ url("posts.edit", { postId: post.getId }) }}">
							E
						</a>
					</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

{% endblock body %}
